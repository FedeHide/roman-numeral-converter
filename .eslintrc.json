{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["standard-with-typescript", "prettier", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "error"
	}
}

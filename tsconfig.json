{
	"compilerOptions": {
		"target": "es2022",
		"module": "ES6",
		"rootDir": "./src/ts",
		"outDir": "./dist/js",
		"sourceMap": true,
		"removeComments": true,
		"noEmitOnError": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	}
}
